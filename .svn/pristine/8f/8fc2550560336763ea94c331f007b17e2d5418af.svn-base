package com.enovell.yunwei.km_micor_service.dto.technicalManagement;

import java.io.Serializable;

public class LineNameDto implements Serializable{

	   
	 /**   
	  * 变量 serialVersionUID : TODO(用一句话描述这个变量表示什么)   
	  */   
	 
	
	private static final long serialVersionUID = -5646978424391212102L;
	/**
	 * Id
	 */
	private String id;

	/**
	 * 线别名称
	 */
	private String name;
	
	/**
	 * 备注
	 */
	private String desc;

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getDesc() {
		return desc;
	}

	public void setDesc(String desc) {
		this.desc = desc;
	}

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}
	@Override
	public String toString() {
		return "LineNameDto [id=" + id + ", name=" + name + ", desc=" + desc + "]";
	}

	
	
}
